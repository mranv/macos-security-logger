{
    "output_dir": "./logs",
    "retention_days": 30,
    "max_file_size": 104857600,
    "save_raw_json": true,
    "json_pretty_print": true,
    "log_patterns": [
        "process == \"securityd\" OR process == \"sandboxd\"",
        "category == \"security\"",
        "subsystem == \"com.apple.security\""
    ]
}
